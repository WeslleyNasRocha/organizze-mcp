{
  "name": "organizze-mcp-server",
  "version": "1.1.1",
  "description": "Model Context Protocol (MCP) server for Organizze API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "organizze-mcp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./index.ts --outdir ./dist --target node",
    "start": "bun run dist/index.js",
    "dev": "bun run --watch index.ts",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "check": "biome check --write .",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "axios": "^1.13.5",
    "zod": "^4.3.6"
  }
}